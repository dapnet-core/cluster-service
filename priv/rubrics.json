{
    "_id": "_design/rubrics",
    "version": 1,
    "views": {
        "byId": {
            "map": "function (doc) {
                emit(doc._id, 1);
            }"
        }
    },
    "filters": {
        "sync": "function (doc, req) { return doc._id.charAt(0) != '_'; }"
    },
    "language": "javascript"
}
